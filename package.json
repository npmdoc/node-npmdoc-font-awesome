{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "font-awesome",
    "description": "api documentation for [font-awesome (v4.7.0)](http://fontawesome.io/) [![npm package](https://img.shields.io/npm/v/npmdoc-font-awesome.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-font-awesome) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-font-awesome.svg)](https://travis-ci.org/npmdoc/node-npmdoc-font-awesome)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "font-awesome": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-font-awesome",
    "keywords": [
        "documentation",
        "font-awesome"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_font_awesome.js",
    "name": "npmdoc-font-awesome",
    "nameAlias": "npmdoc_font_awesome",
    "nameOriginal": "npmdoc-font-awesome",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-font-awesome.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
}
